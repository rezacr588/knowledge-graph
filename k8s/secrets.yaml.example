apiVersion: v1
kind: Secret
metadata:
  name: hybrid-rag-secrets
type: Opaque
stringData:
  neo4j-uri: "neo4j+s://your-instance.databases.neo4j.io"
  neo4j-username: "neo4j"
  neo4j-password: "your-password"
  qdrant-url: "https://your-cluster.qdrant.io:6333"
  qdrant-api-key: "your-api-key"
  gemini-api-key: "your-gemini-api-key"
