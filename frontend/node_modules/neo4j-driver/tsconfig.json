{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true
  },
  "include": ["types/**/*.ts", "test/types/**/*.ts", "*.ts"]
}
